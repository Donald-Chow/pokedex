<div id="<%= dom_id pokemon %>">
  <p>
    <strong>Name:</strong>
    <%= pokemon.name.chars %>
  </p>

  <p>
    <strong>Abilities:</strong>
    <%= pokemon.abilities %>
  </p>

  <p>
    <strong>Height:</strong>
    <%= pokemon.height %>
  </p>

  <p>
    <strong>Weight:</strong>
    <%= pokemon.weight.to_s %>
  </p>

  <p>
    <strong>Types:</strong>
    <%= pokemon.types %>
  </p>

</div>
